{
  "manifest_version": 3,
  "name": "TwitchPoints",
  "description": "It is used to see the total points of the channels you follow.",
  "version": "1.0",
  "icons": {
    "128": "twitchP-128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "host_permissions": [
    "https://developer.chrome.com/*"
  ],
  "permissions": [
    "tabGroups"
  ]
}
